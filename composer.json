{
    "name": "gregpriday/laravel-scout-qdrant",
    "description": "This is my package laravel-scout-qdrant",
    "keywords": [
        "laravel",
        "laravel-scout-qdrant",
        "vector search"
    ],
    "homepage": "https://github.com/gregpriday/laravel-scout-qdrant",
    "license": "MIT",
    "authors": [
        {
            "name": "Greg Priday",
            "email": "greg@siteorigin.com",
            "role": "Developer"
        }
    ],
    "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/gregpriday/qdrant-php"
        }
    ],
    "require": {
        "php": "^8.1",
        "guzzlehttp/guzzle": "^7.7",
        "hkulekci/qdrant": "dev-feature/multiple-vector-support",
        "illuminate/contracts": "^10.0",
        "laravel/scout": "^10.2",
        "openai-php/laravel": "^0.5.0",
        "spatie/laravel-package-tools": "^1.14.0"
    },
    "require-dev": {
        "laravel/pint": "^1.0",
        "nunomaduro/collision": "^7.9",
        "orchestra/testbench": "^8.0",
        "pestphp/pest": "^2.0",
        "pestphp/pest-plugin-arch": "^2.0",
        "pestphp/pest-plugin-laravel": "^2.0",
        "phpunit/phpunit": "^10.1",
        "spatie/laravel-ray": "^1.26"
    },
    "autoload": {
        "psr-4": {
            "GregPriday\\LaravelScoutQdrant\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "GregPriday\\LaravelScoutQdrant\\Tests\\": "tests/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@php ./vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "GregPriday\\LaravelScoutQdrant\\LaravelScoutQdrantServiceProvider"
            ],
            "aliases": {
                "LaravelScoutQdrant": "GregPriday\\LaravelScoutQdrant\\Facades\\LaravelScoutQdrant"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
